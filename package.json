{
  "name": "slate-vue3",
  "version": "0.8.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "slate-react library implemented with vue3",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/slate-vue/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/slate/index.d.ts",
        "default": "./dist/core.js"
      }
    },
    "./dom": {
      "import": {
        "types": "./dist/slate-dom/index.d.ts",
        "default": "./dist/dom.js"
      }
    },
    "./history": {
      "import": {
        "types": "./dist/slate-history/index.d.ts",
        "default": "./dist/history.js"
      }
    },
    "./hyperscript": {
      "import": {
        "types": "./dist/slate-hyperscript/index.d.ts",
        "default": "./dist/hyperscript.js"
      }
    },
    "./yjs": {
      "import": {
        "types": "./dist/slate-yjs/index.d.ts",
        "default": "./dist/yjs.js"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build-all": "vite build --mode=lib",
    "e2e": "playwright test",
    "e2e-ui": "playwright test --ui",
    "test": "vitest"
  },
  "dependencies": {
    "direction": "^2.0.1",
    "is-hotkey": "^0.2.0",
    "lodash-es": "^4.17.21",
    "scroll-into-view-if-needed": "^3.1.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@liveblocks/client": "^3.8.0",
    "@liveblocks/yjs": "^3.8.0",
    "@playwright/test": "^1.55.1",
    "@testing-library/vue": "^8.1.0",
    "@types/is-hotkey": "^0.1.10",
    "@types/is-url": "^1.2.32",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.6.2",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "babel-plugin-transform-regex": "^6.0.1",
    "image-extensions": "^1.1.0",
    "is-url": "^1.2.4",
    "jsdom": "^27.0.0",
    "prismjs": "^1.30.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-slate-transformer": "^0.9.0",
    "typescript": "~5.9.3",
    "unified": "^11.0.5",
    "vite": "^7.1.9",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-router": "^4.5.1",
    "yjs": "^13.6.27"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guan-Erjia/slate-vue3.git"
  },
  "homepage": "https://guan-erjia.github.io/slate-vue3",
  "keywords": [
    "slate-vue3",
    "slate-vue",
    "slate",
    "editor",
    "contenteditable",
    "richtext",
    "rich",
    "editable",
    "markdown",
    "html"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
